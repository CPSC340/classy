{
    "name": "portal-backend",
    "version": "1.0.0",
    "description": "SDMM Student Portal Backend",
    "main": "build/App.js",
    "scripts": {
        "test": "node --require dotenv/config ./node_modules/.bin/mocha --timeout 30000 --recursive",
        "testCI": "node --require dotenv/config ./node_modules/.bin/mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./testOutput/test-results.xml --timeout 5000 --recursive --exit",
        "cover": "nyc --reporter html yarn run test",
        "coverCIworked": "./node_modules/.bin/nyc --reporter html --report-dir ./testOutput --reporter=text-lcov yarn run testCI | coveralls",
        "coverCI": "./node_modules/.bin/nyc --reporter html --report-dir ./testOutput --reporter=text-lcov yarn run testCI",
        "coveralls": "./node_modules/.bin/nyc report --report-dir ./testOutput --reporter=text-lcov | coveralls",
        "codecov": "./node_modules/.bin/nyc report --report-dir ./testOutput --reporter=json > ./testOutput/coverage.json && codecov --token=$CODECOV_TOKEN",
        "build": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ubccpsc/sdmm-portal-backend.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ubccpsc/sdmm-portal-backend/issues"
    },
    "homepage": "https://github.com/ubccpsc/sdmm-portal-backend#readme",
    "dependencies": {
        "@types/csv-parse": "^1.1.11",
        "awesome-typescript-loader": "^3.4.1",
        "child-process-promise": "^2.2.1",
        "client-oauth2": "^4.2.1",
        "codecov": "^3.0.4",
        "coveralls": "^3.0.2",
        "csv-parse": "^2.5.0",
        "dotenv": "^5.0.1",
        "fs-extra": "^5.0.0",
        "mocha": "^5.1.1",
        "mocha-junit-reporter": "^1.17.0",
        "mongodb": "^3.0.3",
        "nyc": "^12.0.2",
        "request": "2.83.0",
        "request-promise-native": "^1.0.5",
        "restify": "^6.3.4",
        "source-map-loader": "^0.2.3",
        "supertest": "^3.0.0",
        "tmp-promise": "^1.0.4"
    }
}
